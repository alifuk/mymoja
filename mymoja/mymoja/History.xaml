        <Label x:Name="label" Content="Datum od:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Datum do:" HorizontalAlignment="Left" Margin="258,9,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="medián" HorizontalAlignment="Left" Margin="112,53,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1_Copy" Content="průměrná odchylka" HorizontalAlignment="Left" Margin="201,53,0,0" VerticalAlignment="Top"/>
