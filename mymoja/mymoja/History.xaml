        <Label x:Name="label1_Copy2" Content="tlak" HorizontalAlignment="Left" Margin="27,113,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lb_medtem" Content="teplota" HorizontalAlignment="Left" Margin="113,84,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1_Copy3" Content="srážky" HorizontalAlignment="Left" Margin="27,144,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lb_medpress" Content="teplota" HorizontalAlignment="Left" Margin="112,115,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lb_medwater" Content="teplota" HorizontalAlignment="Left" Margin="113,144,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lb_avgtemp" Content="teplota" HorizontalAlignment="Left" Margin="201,84,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lb_avgpress" Content="teplota" HorizontalAlignment="Left" Margin="201,115,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lb_avgwater" Content="teplota" HorizontalAlignment="Left" Margin="201,144,0,0" VerticalAlignment="Top"/>
